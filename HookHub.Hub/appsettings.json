{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "URLs": {
    "HookHubNetURL": "http://localhost:5100/HOOKHUBNET"
  },
  "TimeIntervals": {
    "KeepAlive": "60000"
  },
  "HookNames": {
    "HookNameFrom": "HookNameFrom-Hub",
    "HookNameTo": "HookHubNet"
  }
}
